  "author": "Integration Team",
  "category": "integration",
  "dependencies": [],
  "permissions": [
    "read",
    "write"
  ],
  "entry_point": "main.py",
  "config_schema": {
    "type": "object",
    "properties": {
      "enabled": {
        "type": "boolean",
        "default": true
