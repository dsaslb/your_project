{
  "plugin_id": "notification_system",
  "name": "알림 시스템 플러그인",
  "version": "1.0.0",
  "description": "이메일, SMS, 푸시 알림을 통합 관리하는 알림 시스템 플러그인입니다.",
  "author": "Communication Team",
  "category": "communication",
  "dependencies": [],
  "permissions": [
    "read",
    "write"
  ],
  "entry_point": "main.py",
  "config_schema": {
    "type": "object",
    "properties": {
      "enabled": {
        "type": "boolean",
        "default": true
      },
      "debug_mode": {
        "type": "boolean",
        "default": false
      }
    }
  }
}